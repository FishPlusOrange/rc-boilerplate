{
  "name": "@fishplusorange/rc-boilerplate",
  "version": "0.1.0",
  "author": "FishPlusOrange",
  "license": "MIT",
  "description": "A React components boilerplate",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "yarn storybook",
    "dev:ts": "yarn build:ts --watch",
    "dev:sass": "yarn build:sass --watch",
    "build": "yarn clean && yarn build:ts && yarn build:sass",
    "build:ts": "tsc",
    "build:sass": "node-sass src/styles/index.scss dist/index.css",
    "clean": "rimraf dist",
    "lint": "eslint --ext ts,tsx src",
    "prettier": "prettier --write src",
    "release": "sh scripts/release.sh",
    "storybook": "start-storybook -p 6006"
  },
  "peerDependencies": {
    "react": ">= 16.8.0",
    "react-dom": ">= 16.8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@storybook/addon-actions": "^5.3.19",
    "@storybook/addon-links": "^5.3.19",
    "@storybook/addons": "^5.3.19",
    "@storybook/preset-scss": "^1.0.2",
    "@storybook/preset-typescript": "^3.0.0",
    "@storybook/react": "^5.3.19",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.7",
    "@typescript-eslint/eslint-plugin": "^2.31.0",
    "@typescript-eslint/parser": "^2.31.0",
    "babel-loader": "^8.1.0",
    "css-loader": "^3.5.3",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "fork-ts-checker-webpack-plugin": "^4.1.5",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.2",
    "node-sass": "^4.14.1",
    "prettier": "^2.0.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "rimraf": "^3.0.2",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.2.1",
    "ts-loader": "^7.0.5",
    "typescript": "^3.8.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": "eslint",
    "src/**/*": "prettier --write"
  },
  "keywords": [
    "react",
    "typescript",
    "sass",
    "react components",
    "boilerplate"
  ],
  "homepage": "https://github.com/FishPlusOrange/rc-boilerplate#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/FishPlusOrange/rc-boilerplate.git"
  },
  "bugs": {
    "url": "https://github.com/FishPlusOrange/rc-boilerplate/issues"
  }
}
