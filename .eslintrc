{
  "env": {
    "browser": true
  },
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "sourceType": "module"
  },
  "plugins": ["prettier", "@typescript-eslint", "react"],
  "extends": ["prettier", "prettier/@typescript-eslint", "prettier/react"],
  "rules": {
    "prettier/prettier": "error"
  }
}
